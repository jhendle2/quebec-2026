Delim(Space  , ' ')
Delim(Newline, '\n')
Delim(Tab    , '\t')
Delim(Tilde, '~')
Delim(Backtick, '`')
Delim(Exclamation, '!')
Delim(At, '@')
Delim(Hash, '#')
Delim(Dollar, '$')
Delim(Percent, '%')
Delim(Caret, '^')
Delim(Ampersand, '&')
Delim(Asterisk, '*')
Delim(ParenOpen, '(')
Delim(ParenClose, ')')
Delim(Minus, '-')
Delim(Plus, '+')
Delim(Equals, '=')
Delim(BraceOpen, '{')
Delim(BracketOpen, '[')
Delim(BraceClose, '}')
Delim(BracketClose, ']')
Delim(Bar, '|')
Delim(Backslash, '\\')
Delim(Colon, ':')
Delim(Semicolon, ';')
Delim(QuoteDouble, '\"')
Delim(QuoteSingle, '\'')
Delim(Less, '<')
Delim(Comma, ',')
Delim(Greater, '>')
Delim(Period, '.')
Delim(Question, '?')
Delim(Slash, '/')